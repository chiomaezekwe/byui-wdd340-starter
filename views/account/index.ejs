<% /* Account Management View */ %>

<h1>Account Management</h1>

<% if (message) { %>
  <div class="flash-message">
    <%= message %>
  </div>
<% } %>

<% if (errors && errors.length > 0) { %>
  <div class="error-box">
    <ul>
      <% errors.forEach(error => { %>
        <li><%= error.msg %></li>
      <% }) %>
    </ul>
  </div>
<% } %>

<p>You're logged in.</p>